  22689 \div 2 = 11344 \text{ Remainder } 1 \\
  11344 \div 2 = 5672 \text{ Remainder } 0 \\
  5672 \div 2 = 2836 \text{ Remainder } 0 \\
  2836 \div 2 = 1418 \text{ Remainder } 0 \\
  1418 \div 2 = 709 \text{ Remainder } 0 \\
  709 \div 2 = 354 \text{ Remainder } 1 \\
  354 \div 2 = 177 \text{ Remainder } 0 \\
  177 \div 2 = 88 \text{ Remainder } 1 \\
  88 \div 2 = 44 \text{ Remainder } 0 \\
  44 \div 2 = 22 \text{ Remainder } 0 \\
  22 \div 2 = 11 \text{ Remainder } 0 \\
  11 \div 2 = 5 \text{ Remainder } 1 \\
